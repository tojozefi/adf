{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adftojo"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/batch')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "primes",
						"type": "Custom",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"command": "/bin/bash run.sh",
							"resourceLinkedService": {
								"referenceName": "AdfBlobStorage",
								"type": "LinkedServiceReference"
							},
							"folderPath": "primes",
							"referenceObjects": {
								"linkedServices": [],
								"datasets": [
									{
										"referenceName": "ds_input",
										"type": "DatasetReference",
										"parameters": {
											"sastoken": "?sv=2019-12-12&ss=b&srt=sco&sp=rwdlacx&se=2020-11-10T08:10:25Z&st=2020-10-09T23:10:25Z&spr=https&sig=reFFC3PIHQcOp%2BQLkJJvQgV%2BRBaDlst7lhDlKMpv7as%3D"
										}
									},
									{
										"referenceName": "ds_output",
										"type": "DatasetReference",
										"parameters": {
											"sastoken": "?sv=2019-12-12&ss=b&srt=sco&sp=rwdlacx&se=2020-11-10T08:10:25Z&st=2020-10-09T23:10:25Z&spr=https&sig=reFFC3PIHQcOp%2BQLkJJvQgV%2BRBaDlst7lhDlKMpv7as%3D"
										}
									}
								]
							}
						},
						"linkedServiceName": {
							"referenceName": "ls_batch",
							"type": "LinkedServiceReference"
						}
					}
				],
				"parameters": {
					"inputFile": {
						"type": "string"
					},
					"inputFolder": {
						"type": "string"
					}
				},
				"annotations": []
			},
			"dependsOn": []
		}
	]
}